{
    "manifest_version": 3,
    "name": "HTML Logger",
    "version": "1.0",
    "description": "Logs the HTML of the current page to the console",
    "permissions": [
      "activeTab",
      "scripting",
      "storage",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "src/logo.png"
    }
  }